<header>
  <h1><%= title %></h1>
  <h3><i><%= subtitle %></i> </h3>
</header>

<nav>
  Links go here
</nav>

<% articles.each do |article| %>
  <article>
    <h2>
      <A HREF = "article/<%= article.title %>"> <%= article.title %> </A>
    </h2>
    <p> <%= article.body %> </p>
  </article>
<% end %>

<footer>
  <p><small>
    <%= copyright %>
  </small><p>
</footer>